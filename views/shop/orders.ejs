<%- include('../includes/head.ejs') %>
</head>

<body>
  <%- include('../includes/navigation.ejs') %>
  <main>
    <% if (orders.length <= 0) { %>
      <h1>Nothing there!</h1>
    <% } else { %>
      <ul>
        <% for (const o of orders) { %>
          <li>
            <h1># <%= o._id %></h1>
            <ul>
              <% for (const p of o.items) { %>
                <li><%= p.title %> (<%= p.quantity %>)</li>
              <% } %>
            </ul>
          </li>
        <% } %>
      </ul>
    <% } %>
  </main>
  <%- include('../includes/end.ejs') %>